{
  "name": "blessing-skin-server",
  "version": "3.4.0",
  "description": "A web application brings your custom skins back in offline Minecraft servers.",
  "repository": {
    "type": "git",
    "url": "https://github.com/printempw/blessing-skin-server"
  },
  "author": "printempw",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "dev": "parcel watch -d public resources/assets/src/components/index.js",
    "build": "parcel build -d public --no-source-maps resources/assets/src/components/index.js && yarn build:polyfill",
    "build:polyfill": "parcel build -d public --no-source-maps resources/assets/src/js/polyfill.js",
    "lint": "eslint --ext=.js,.vue -f=beauty .",
    "release": "gulp zip",
    "test": "jest",
    "codecov": "codecov -F js"
  },
  "dependencies": {
    "admin-lte": "^2.4.2",
    "axios": "^0.18.0",
    "bootstrap": "^3.3.7",
    "bootstrap-fileinput": "^4.4.7",
    "chart.js": "^2.7.1",
    "datatables.net-bs": "^1.10.16",
    "es6-promise": "^4.2.4",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.12.0",
    "icheck": "^1.0.2",
    "jqPaginator": "^1.2.0",
    "jquery": "^3.3.1",
    "lodash.debounce": "^4.0.8",
    "regenerator": "^0.12.3",
    "skinview3d": "^1.1.0-alpha.4",
    "sweetalert2": "^6.11.5",
    "toastr": "^2.1.4",
    "vue": "^2.5.16",
    "vuejs-paginate": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/jquery": "^3.3.5",
    "@vue/component-compiler-utils": "^2.1.0",
    "@vue/test-utils": "^1.0.0-beta.21",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "babel-plugin-transform-remove-console": "^6.9.0",
    "codecov": "^3.0.0",
    "del": "^3.0.0",
    "eslint": "^5.2.0",
    "eslint-formatter-beauty": "^3.0.0-beta.2",
    "eslint-plugin-vue": "^5.0.0-beta.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.3",
    "gulp-batch-replace": "^0.0.0",
    "gulp-clean-css": "^3.9.2",
    "gulp-concat": "^2.6.1",
    "gulp-notify": "^3.2.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-stylus": "^2.7.0",
    "gulp-uglify": "^3.0.0",
    "gulp-zip": "^4.1.0",
    "jest": "^23.4.1",
    "jest-extended": "^0.7.2",
    "merge2": "^1.2.1",
    "parcel-bundler": "^1.9.7",
    "run-sequence": "^2.2.1",
    "stylus": "^0.54.5",
    "vue-hot-reload-api": "^2.3.0",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.16"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.vue$": "vue-jest"
    },
    "moduleFileExtensions": [
      "js",
      "vue",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/resources/assets/src/components/$1"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/resources/assets/tests/setup.js",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "setup"
    ],
    "testRegex": "resources/assets/tests/.*\\.(spec|test)\\.js"
  }
}
