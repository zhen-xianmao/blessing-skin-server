{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "useBuiltIns": "usage"
    }]
  ],
  "plugins": [
    "@babel/syntax-dynamic-import",
    "@babel/proposal-optional-catch-binding"
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/env", {
          "targets": { "node": "current" },
          "ignoreBrowserslistConfig": true
        }]
      ]
    }
  }
}
