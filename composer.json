{
  "name": "printempw/blessing-skin-server",
  "description": "A web application brings your custom skins back in offline Minecraft servers.",
  "license": "GPL-3.0",
  "require": {
    "php": ">=7.1.3",
    "filp/whoops": "^2.1",
    "predis/predis": "~1.0",
    "erusev/parsedown": "^1.6",
    "swiggles/memcache": "^2.0",
    "doctrine/inflector": "1.1.0",
    "laravel/framework": "5.6.*",
    "nesbot/carbon": "^1.32.0",
    "devitek/yaml-translation": "^4.1.0",
    "printempw/laravel-datatables-lite": "^1.0",
    "composer/semver": "^1.4",
    "mews/captcha": "^2.2"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "^1.1.0",
    "phpdocumentor/reflection-docblock": "3.2.2",
    "phpunit/phpunit": "~7.0",
    "laravel/browser-kit-testing": "~4.0",
    "barryvdh/laravel-debugbar": "^3.1.5",
    "league/flysystem-memory": "^1.0",
    "mikey179/vfsStream": "^1.6.5",
    "barryvdh/laravel-ide-helper": "^2.4"
  },
  "autoload": {
    "classmap": [
      "app/Models",
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
